            <ITEXT CH="# GRADIENT FILL"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="# By Jaseman - 8th August 2012"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="import os, pygame; from pygame.locals import *"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="pygame.init(); clock = pygame.time.Clock()"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="os.environ['SDL_VIDEO_WINDOW_POS'] = 'center'"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="pygame.display.set_caption(&quot;Gradient Fill&quot;)"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="screen=pygame.display.set_mode([600,382],0,32)"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="# We know that color values are between 0-255 so..."/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="# Lets give the sky surface 255 pixels height"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="sky = pygame.Surface((600,255))"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="# Now we will draw rectangles down from the top of the sky surface"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="# Each rectangle gets slightly higher color values so that the blue"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="# gets lighter and lighter towards the horizon"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="# We will define some variables and then create a loop to draw"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="# rectangles"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="r=0; g=64; b=128 # Start Red Green and Blue Values"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="for l in range (0,255):"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <tab/>
            <ITEXT CH="pygame.draw.rect(sky,(r,g,b),(0,l-1,600,l))"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <tab/>
            <ITEXT CH="r=r+1;g=g+1;b=b+1 # Increase the Red Green and Blue Values"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <tab/>
            <ITEXT CH="if r&gt;=255: r=255 # Max amount of red allowed"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <tab/>
            <ITEXT CH="if g&gt;=255: g=255 # Max amount of green allowed"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <tab/>
            <ITEXT CH="if b&gt;=255: b=255 # Max amount of blue allowed"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="# Let's do a similar thing with the ground"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="# For the ground let's have half as many pixels height"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="# as available color values (256/2=128)"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="ground = pygame.Surface((600,128))"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="r=192; g=255; b=192 # Start Red Green and Blue Values"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="for l in range (0,128):"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <tab/>
            <ITEXT CH="pygame.draw.rect(ground,(r,g,b),(0,l-2,600,l))"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <tab/>
            <ITEXT CH="r=r-2;g=g-2;b=b-2 # Decrease the Red Green and Blue Values"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <tab/>
            <ITEXT CH="if r&lt;=0: r=0 # Min amount of red allowed"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <tab/>
            <ITEXT CH="if g&lt;=0: g=0 # Min amount of green allowed"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <tab/>
            <ITEXT CH="if b&lt;=0: b=0 # Min amount of blue allowed"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <tab/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="screen.blit(sky,(0,0)) # Paste the sky surface onto the screen"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="screen.blit(ground,(0,255)) # Paste the ground surface onto the screen"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT CH="pygame.display.update()"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <para PARENT="Code Text" LINESP="11.78"/>
            <ITEXT FONT="Courier 10 Pitch Bold" FONTSIZE="10" FCOLOR="Grey Mid" FSHADE="100" CH="pygame.time.wait(10000) # A 10 second pause before the program ends"/>
            <trail ALIGN="0" LINESP="11.78"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
